# HTTP
PORT=8080
HTTP_READ_TIMEOUT_SEC=5
HTTP_WRITE_TIMEOUT_SEC=5
HTTP_IDLE_TIMEOUT_SEC=30
REQUEST_TIMEOUT_MS=300

# PostgreSQL
DATABASE_URL=postgres://postgres:postgres@db:5432/app?sslmode=disable
PG_MAX_CONNS=24
PG_HEALTHCHECK_PERIOD_SEC=30
PG_MAX_CONN_IDLE_TIME_SEC=300
PG_MAX_CONN_LIFETIME_SEC=0

# Redis
REDIS_ADDR=redis:6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_DB=0
REDIS_DIAL_TIMEOUT_MS=100
REDIS_READ_TIMEOUT_MS=100
REDIS_WRITE_TIMEOUT_MS=100
REDIS_POOL_SIZE=48
REDIS_MIN_IDLE_CONNS=8
REDIS_POOL_TIMEOUT_MS=200
REDIS_MAX_RETRIES=1
REDIS_CONN_MAX_IDLE_SEC=300

# Cache
CACHE_TTL_SECONDS=3600